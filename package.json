		"@vue/apollo-composable": "^4.0.0-alpha.17",
		"bootstrap": "^5.1.3",
		"graphql-ws": "^5.8.2",
		"jquery": "^3.6.0",
		"pinia": "^2.0.13",
		"subscriptions-transport-ws": "^0.11.0",
		"uuid": "^8.3.2",
